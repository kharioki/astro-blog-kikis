---
// layout component import
import MainHead from './MainHead.astro';

// style import
import '../styles/global.css';

interface Props {
  title?: string;
  description?: string;
}

const { title = "Kiki's Blog", description = "My blog" } = Astro.props as Props;
---

<html lang="en">
	<MainHead {title} {description} />
	<body>
		<slot>
      Default Text
    </slot>
	</body>
</html>
